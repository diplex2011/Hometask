class Boxer:
	def __init__(self, name, age, weight, height):
	    self.name = name
	    self.age = age
	    self.weight = weight
	    self.height = height

>>> vasily = Boxer(name='vasily', age='22', weight='100', height='180')
>>> def kick(self):
	return "I'll kick you!"

>>> def pull_up(self):
	return "I can make 50 pull_ups!"
